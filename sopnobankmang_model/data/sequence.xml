<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for customer regestration-->
        <record id="send_sms_sequence" model="ir.sequence">
            <field name="name">Send Message</field>
            <field name="code">send.sms.sequence</field>
            <field name="prefix">SCL</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="customer_payment_sequence" model="ir.sequence">
            <field name="name">Customer Payment</field>
            <field name="code">customer.payment.sequence</field>
            <field name="prefix">PAY</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>